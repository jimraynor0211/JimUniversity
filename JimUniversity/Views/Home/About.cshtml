@model IEnumerable<JimUniversity.ViewModels.EnrollmentDateGroup>

@{
    ViewBag.Title = "Student Body Statistics";
    ViewBag.Current = "about";
}

<div class="box box-primary">
    <div class="box-body">
        <h2>@ViewBag.Title.</h2>
        
        <table>
            <tr>
                <th>
                    Enrollment Date
                </th>
                <th>
                    Students
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EnrollmentDate)
                    </td>
                    <td>
                        @item.StudentCount
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
        
